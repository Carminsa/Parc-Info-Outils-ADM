<div class="col-md-12 col-md-pull-1">

    <table class="table">

        <thead>
        <tr>
            <th>#</th>
            <th>Login</th>
            <th>Prénom</th>
            <th>Nom de Famille</th>
            <th>Email</th>
            <th>Téléphone</th>
            <th>Rôle</th>
            <th>Action</th>
        </tr>
        </thead>

        <% for(var i = 0 ; i < users.length ; i++) {%>
        <tr>
            <td><%= users[i].id %></td>
            <td><%= users[i].login %></td>
            <td><%= users[i].firstname %></td>
            <td><%= users[i].lastname %></td>
            <td><%= users[i].mail %></td>
            <td><%= users[i].phone %></td>
            <td><%= users[i].role %></td>
            <td><a href="/users/<%= users[i].id %>">Voir</a></td>
        </tr>
        <% } %>
    </table>


</div>